#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, x, y;
    cout << "n= ";
    cin >> n;

    vector<int> v1;
    vector<int> v2;

    for (int i = 0; i < n; i++) {
        cin >> x;
        v1.push_back(x);
    }
    for (int i = 0; i < n; i++) {
        cin >> y;
        v2.push_back(y);
    }

    vector<int> sum(n);
    for (int i = 0; i < n; i++) {
        sum[i] = v1[i] + v2[i];
    }
    for (int i = 0; i < n; i++) {
        cout << sum[i] << " ";
    }

    return 0;
}
