#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, x;
    cin >> n;

    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        cin >> v[i];
    }
    
    int max_val = v[0];
    int max_index = 0;

    for (int i = 1; i < n; i++) {
        if (v[i] > max_val) {
            max_val = v[i];
            max_index = i;
        }
    }
    cout << "Index=" << max_index << endl;

    return 0;
}
