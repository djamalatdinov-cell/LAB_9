#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> v;
    int n=0, even=0, odd = 0;
    while (n>=0) {
        cin>>n;
        v.push_back(n);
        if (n%2==0) {
            even++;
        } else {
            odd++;
        }
    }
    cout<<"even= "<<even<<endl;
    cout<<"odd= "<<odd<<endl;
    return 0;
}
